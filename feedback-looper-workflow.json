        "channel": "engineering-bugs",
        "text": "üö® New Bug Reported:\nSummary: {{$json[\"summary\"]}}\nOriginal: {{$json[\"body\"][\"message\"]}}"
      },
      "name": "Slack Engineering",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [700, 200]
    },
    {
      "parameters": {
        "channel": "customer-love",
        "text": "‚ù§Ô∏è New Praise:\nSummary: {{$json[\"summary\"]}}"
      },
      "name": "Slack Sales",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 1,
      "position": [700, 400]
    }
  ],
  "connections": {
    "Webhook (Typeform)": { "main": [[{ "node": "OpenAI Analysis", "type": "main", "index": 0 }]] },
    "OpenAI Analysis": { "main": [[{ "node": "If Bug", "type": "main", "index": 0 }]] },
    "If Bug": {
      "main": [
        [{ "node": "Slack Engineering", "type": "main", "index": 0 }],
